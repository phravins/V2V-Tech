{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/index.cjs"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "builds": [
        {
            "src": "server/index.cjs",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ]
}